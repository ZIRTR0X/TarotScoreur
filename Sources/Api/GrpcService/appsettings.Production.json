{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Nlog": {
    "rules": [
      {
        "logger": "*",
        "minlevel": "Info",
        "writeTo": "lifetimeConsole"
      }
    ]
  }
}
