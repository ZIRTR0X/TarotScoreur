{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Nlog": {
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "maxLevel": "Fatal",
        "writeTo": "lifetimeConsole"
      }
    ]
  }
}
